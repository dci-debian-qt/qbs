--- qbs.orig/src/lib/corelib/use_installed_corelib.pri	2015-03-12 18:53:43.451299470 -0500
+++ qbs/src/lib/corelib/use_installed_corelib.pri	2015-03-12 18:54:43.995297832 -0500
@@ -4,11 +4,9 @@
 
 QBSLIBDIR=$${PWD}/../../lib
 unix {
-    LIBS += -L$$QBSLIBDIR -lqbscore
